<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="personTable" prefWidth="350.0"
           xmlns="http://javafx.com/javafx/8.0.121"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.bars.test.client.controllers.TableController">
    <columns>
        <TableColumn fx:id="firstname" prefWidth="100.0" text="Фамилия"/>
        <TableColumn fx:id="lastname" prefWidth="100.0" text="Имя"/>
        <TableColumn fx:id="patronymic" prefWidth="100.0" text="Отчество"/>
        <TableColumn fx:id="position" prefWidth="100.0" text="Должность"/>
    </columns>
</TableView>
